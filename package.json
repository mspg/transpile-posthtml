{
  "name": "@mspg/transpile-posthtml",
  "version": "0.0.1",
  "description": "posthtml transpiler for @mspg",
  "main": "src/index.js",
  "scripts": {
    "start": "t -p",
    "test": "t",
    "format": "f -w",
    "format:check": "f",
    "calls": "nyc report --reporter=text-lcov | calls",
    "build": "NODE_ENV=production mspg build zip",
    "publish": "NODE_ENV=production mspg build zip publish",
    "connect": "mspg connect",
    "dev": "mspg build watch serve"
  },
  "dependencies": {
    "@magic/log": "0.1.0-cjs",
    "@magic/types": "0.0.5",
    "html-minifier": "4.0.0",
    "posthtml": "0.11.4",
    "posthtml-expressions": "1.1.0",
    "posthtml-extend": "0.3.0",
    "posthtml-hint": "github:jaeh/posthtml-hint",
    "posthtml-include": "1.2.0",
    "posthtml-mixins": "1.0.1"
  },
  "devDependencies": {
    "@magic/test": "0.1.42",
    "@mspg/core": "github:mspg/core"
  },
  "files": [
    "src"
  ],
