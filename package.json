{
  "name": "@mspg/transpile-posthtml",
  "version": "0.0.1",
  "description": "posthtml transpiler for @mspg",
  "main": "src/index.js",
  "scripts": {
    "start": "t -p",
    "test": "t",
    "format": "f -w",
    "format:check": "f",
